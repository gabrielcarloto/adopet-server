// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pet {
  createdAt DateTime @default(now())
  id        String   @id @default(uuid())
  name      String
  image     String
  location  String
  breed     String
  age       String
  size      String
  behaviour String
}
